    "bitcoin login",
    "blockchain",
    "blockchain auth",
    "blockchain authentication",
    "blockchain id",
    "blockchain login",
    "blockchainid",
    "blockstack",
    "blockstack auth",
    "command",
    "crypto",
    "cryptography",
    "ethereum",
    "id",
    "identity",
    "login",
    "profile",
    "signin",
    "sso",
    "stacks",
    "token"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirosystems/stacks.js.git"
  },
  "bugs": {
    "url": "https://github.com/blockstack/blockstack.js/issues"
